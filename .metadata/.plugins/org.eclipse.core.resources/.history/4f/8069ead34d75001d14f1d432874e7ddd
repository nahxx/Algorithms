package stage2;

import java.util.Scanner;

public class Main05 {

	public static void main(String[] args) {
		// 45분 일찍
		Scanner sc = new Scanner(System.in);
		int h = sc.nextInt();
		int m = sc.nextInt();
		
		if(m == 45) {
			m = 0;
			h--;
		} else if(m < 45) {
			m = 60 - (45 - m);
			h--;
		} else {
			m -= 45;
		}
		if(h == -1) {
			h = 23;
		}
		System.out.println(h + " " + m);
	}

}
